package week3;

import java.util.Scanner;

public class ContinueEx {

	public static void main(String[] args) {
		int num, sum, nu;
		sum = 0;
		num = 1;
		Scanner a = new Scanner(System.in);
		System.out.print("수를 입력하세요: ");
		nu = a.nextInt();
		while(true) {
			if(num%2==0) {
				System.out.println(num);
			}

			num++;
			if(num>nu) {
				break;
			}
		}

	}
}
